package com.smallcultfollowing.lathos.server;

import java.util.LinkedList;
import java.util.List;

import com.smallcultfollowing.lathos.model.ContentItem;
import com.smallcultfollowing.lathos.model.Output;
import com.smallcultfollowing.lathos.model.Page;

public class PageImpl implements Page {
	
	public final String id;
	private final LinkedList<ContentItem> contents = new LinkedList<ContentItem>();
	private final LinkedList<Page> embeddedIn = new LinkedList<Page>();

	@Override
	public synchronized void addedTo(Page page) {
		
	}

	@Override
	public void render(Output out) {
		// TODO Auto-generated method stub

	}

	@Override
	public String getId() {
		return id;
	}

	@Override
	public List<ContentItem> contents() {
		return null;
	}

	@Override
	public synchronized void addContent(ContentItem content) {
		contents.add(content);
		content.addedTo(this);
	}

}
