package com.smallcultfollowing.lathos.server;

import com.smallcultfollowing.lathos.model.Output;
import com.smallcultfollowing.lathos.model.Page;
import com.smallcultfollowing.lathos.model.PageContent;

public class Breadcrumbs implements PageContent {
	
	public final Page page;
	public final Breadcrumbs previous;
	
	public Breadcrumbs(Page page, Breadcrumbs previous) {
		super();
		this.page = page;
		this.previous = previous;
	}

	@Override
	public void render(Output out) {
		if(out.topPage() == page) {
			if(previous != null) {
				previous.renderInternal(out);
			}
		}
	}
	
	public void renderInternal(Output out) {
		
	}

}
