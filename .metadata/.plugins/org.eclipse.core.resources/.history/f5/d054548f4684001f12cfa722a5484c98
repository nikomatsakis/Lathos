import com.smallcultfollowing.lathos.model.Page;

public class Text implements ContentItem {
	
	private final String text;
	
	public Text(String text) {
		super();
		this.text = text;
	}

	@Override
	public void addedTo(Page page) {
	}

	@Override
	public void renderEmbedded(Output out) {
	}

}
